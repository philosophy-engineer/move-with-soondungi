{
  "name": "soondungi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "lefthook install"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{ts,tsx,js,jsx}": [
      "eslint --max-warnings 0"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "lefthook": "^2.1.1",
    "lint-staged": "^16.2.7",
    "prettier": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:"
  },
  "pnpm": {
    "overrides": {
      "turbo": "2.8.10",
      "@turbo/gen": "2.8.10",
      "@eslint/js": "9.39.2",
      "eslint-plugin-turbo": "2.8.10",
      "eslint": "9.39.2",
      "prettier": "3.8.1",
      "typescript": "5.9.3",
      "react": "19.2.4",
      "react-dom": "19.2.4",
      "@types/react": "19.2.10",
      "@types/react-dom": "19.2.3",
      "next-themes": "0.4.6",
      "lucide-react": "0.574.0",
      "zod": "4.3.6"
    }
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  }
}
